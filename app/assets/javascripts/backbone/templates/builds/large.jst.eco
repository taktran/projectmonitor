<article class="build large <%= if @success then 'success' else 'failure'%>">
<h1 class="name">
  <%= @name %>
</h1>
<% if !@aggregate: %>
  <aside class='history'>
    <ol class="statuses">
      <% for status in @statuses: %>
        <li class="<%= if status then 'success' else 'failure' %>" />
      <% end %>
    </ol>
    <div class="last-build"><%= @last_build %></time>
  </aside>
<% end %>
</article>

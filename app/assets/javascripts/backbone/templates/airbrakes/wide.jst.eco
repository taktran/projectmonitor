<article class="airbrake wide <%= if @success then 'success' else 'failure'%>">
  <div class="title">Airbrake</div>
  <dl class="unresolved-errors">
    <dt>Unresolved errors</dt>
    <dd class="error-count"><%= @error_count %></dd>
  </dl>
  <dl class="last-error">
    <dd><%= @last_error %></dd>
  </dl>
</article>
